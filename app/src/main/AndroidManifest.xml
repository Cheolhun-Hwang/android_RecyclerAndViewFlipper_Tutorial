<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hchooney.qewqs.recycler_tutorial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Recycler.Exam1.RecyclerExam1Activity" />
        <activity android:name=".Recycler.Exam2.RecyclerExam2Activity" />
        <activity android:name=".Recycler.Exam3.RecyclerExam3Activity" />
        <activity android:name=".Recycler.Exam4.RecyclerExam4Activity"></activity>
    </application>

</manifest>